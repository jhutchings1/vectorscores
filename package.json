{
  "name": "vectorscores",
  "version": "2.0.0",
  "description": "A series of new compositions built on the web",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npm run clean && npm run copy && eleventy",
    "build:debug": "npm run clean && npm run copy && DEBUG=Eleventy* eleventy",
    "clean": "rm -rf _site",
    "copy": "mkdir -p _includes/docs && cp docs/README.md _includes/docs/README.md",
    "lint:fix": "prettier --write {.,pages}/*.js",
    "serve": "npm run clean && npm run copy && eleventy --serve --port=8081",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnteske/vectorscores.git"
  },
  "keywords": [
    "music",
    "composition",
    "d3"
  ],
  "author": "John Teske",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnteske/vectorscores/issues"
  },
  "homepage": "https://github.com/johnteske/vectorscores#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.7.0"
  },
  "devDependencies": {
    "prettier": "^1.15.3"
  }
}
